{
	"name": "echoes-of-deep",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "bun run generate-content.ts && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"generate-content": "bun run generate-content.ts"
	},
	"devDependencies": {
		"@humanspeak/svelte-markdown": "^0.8.10",
		"@sveltejs/adapter-node": "^5.3.1",
		"@sveltejs/kit": "^2.31.1",
		"@sveltejs/vite-plugin-svelte": "^6.1.2",
		"@types/bun": "^1.2.20",
		"glob": "^11.0.3",
		"svelte": "^5.38.1",
		"svelte-adapter-bun": "^0.5.2",
		"svelte-check": "^4.3.1",
		"typescript": "^5.9.2",
		"vite": "^7.1.2"
	},
	"dependencies": {
		"gray-matter": "^4.0.3",
		"marked": "^16.1.2"
	}
}